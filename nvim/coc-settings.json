{
  "tsserver.enable": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.suggestionActions.enabled": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
  "snippets.userSnippetsDirectory": "~/dotfiles/nvim/snippets",
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "suggest.noselect": false,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "suggest.enablePreselect": true,
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.colorSupport": true,
  "tsserver.formatOnType": true,
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.jumpCommand": "vsplit",
  "diagnostic.floatConfig": {
    "border": false,
    "enableMessage": false,
    "focusable": false
  },
  "diagnostic.enableMessage": "never",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.messageTarget": "none",
  "diagnostic.enablePopupHover": false,
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.requireConfig": true,
  "prettier.onlyUseLocalVersion": true,
  "prettier.disableSuccessMessage": true,
  "prettier.formatterPriority": 1,
  "snippets.ultisnips.pythonPrompt": false
}